program test_one;
    var b, i, fact, primer_numero, segundo_numero, temp :int;
        a, e :float;

    void fibonacci(n: int) [
        var primer_numero, segundo_numero, temp: int;
        {
            segundo_numero = 1;
            while (n > 0) do {
                temp = primer_numero + segundo_numero;
                primer_numero = segundo_numero;
                segundo_numero = temp;
                n = n - 1;
            }
            print(segundo_numero);

        }
    ];

    
    void factorial(n: int) [
        {
            fact = 1;
            while (n > 0) do {
                fact = fact * n;
                n = n - 1;
            }
        }
    ];

main {

        factorial(10);
        print("Factorial de ", 10, " es: ", fact);
        fibonacci(20);
    }
end